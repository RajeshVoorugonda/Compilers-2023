func int Pythagoreantriplet() {
    var int m;
    var int n;
    var int a;
    var int b;
    var int c;
    for (m = 1; m <= 22; m=m+1) {
        for (n = 1; n < m; n=n+1) {
            if (m*(m+n) == 500) {
                a = m*m - n*n;
                b = 2*m*n;
                c = m*m + n*n;
                return a*b*c; 
            }
        }
    }
}

var int answer = Pythagoreantriplet();

zout(answer);