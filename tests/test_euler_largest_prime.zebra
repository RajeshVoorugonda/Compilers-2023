var int in = 13;

func int largestPrimeFactor(int n){
    var int factor = 2;
    while (factor * factor <= n) {
        if (n % factor == 0) {
            n = n // factor;
        }
        else {
            factor = factor + 1;
        }
    }
    return n;
}

var int out = largestPrimeFactor(in);

