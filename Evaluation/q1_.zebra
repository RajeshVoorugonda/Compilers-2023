var int input_number_ = 19;
var int flag = 0;

var int input_number = input_number_;
if (input_number < 0){
     input_number = input_number * -1;
}

if (input_number == 1){
     zout(input_number_, "it is neither prime nor composite");
}
else{
     for (var int  i = 2; i<input_number; i=i+1){
          if (input_number % i == 0){
               flag = 1;
          }
     }

     if (flag == 0){
          zout(input_number_," it is a prime number");
     }


     else{
          zout(input_number_, "it is a composite number");
     }
}

